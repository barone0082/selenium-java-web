import org.openqa.selenium.remote.DesiredCapabilities;


public class qsssdsds {

	public static void main(String[] args) {
		ChromeOptions dc = new ChromeOptions();
	   // dc.setCapability("chrome.binary", "C:\\Program Files\\ChromeDriver\\chromedriver.exe");
		dc.setBinary("C:\\Program Files\\ChromeDriver\\chromedriver.exe");
	    ChromeDriver driver = new ChromeDriver(dc);
		//WebDriver driver = new ChromeDriver();
		driver.get("www.google.com");

	}

}


